  "scripts": {
    "postinstall": "react-native link",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "logs": "docker-compose logs",
    "rm": "docker rm -f $(docker ps -a -q)",
    "clean": "docker rmi -f $(docker images -qf dangling=true)",
    "build": "webpack && docker-compose build",
    "rebuild": "npm stop && docker rmi -f shitulv/mongodb shitulv/api shitulv/graphics && npm run build && npm start",
    "graphics": "npm stop && docker rmi -f shitulv/graphics && webpack && docker-compose build graphics && npm start",
    "api": "npm stop && docker rmi -f shitulv/api && webpack && docker-compose build api && npm start",
    "push": "docker push shitulv/mongodb && docker push shitulv/api && docker push shitulv/graphics",
    "ios": "react-native run-ios",
    "lint": "esw webpack.config.* src"
  },
