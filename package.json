{
  "name": "trails",
  "version": "0.7.1",
  "description": "",
  "keywords": [],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yeegr/trails.git"
  },
  "author": "Stone Chen",
  "license": "ISC",
  "config": {
    "registry": "registry.cn-hangzhou.aliyuncs.com"
  },
  "scripts": {
    "postinstall": "react-native link",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "logs": "docker-compose logs",
    "kill": "docker rm -f $(docker ps -aq)",
    "clean": "docker rmi -f $(docker images -qf dangling=true)",
    "empty": "docker rmi -f $(docker images)",
    "build": "webpack && docker-compose build",
    "static": "webpack && docker restart static",
    "api": "webpack && docker restart api",
    "lint": "esw webpack.config.* src",
    "ios": "react-native run-ios --simulator='iPhone 6 Plus'",
    "dev": "npm start && npm run ios",

    "codepush": "code-push release-react trails ios",
    "pkg:env": "export APP_ENV=staging && export DOCKER_REPO=registry.cn-hangzhou.aliyuncs.com",
    "pkg:build": "docker-compose -f dc.staging.yml build",
    "pkg:push": "docker push $npm_package_config_registry/shitulv/mongodb && docker push $npm_package_config_registry/shitulv/static && docker push $npm_package_config_registry/shitulv/api",
    "pkg:clean": "docker rmi $npm_package_config_registry/shitulv/mongodb $npm_package_config_registry/shitulv/static $npm_package_config_registry/shitulv/api",
    "package": "npm run pkg:env && npm run pkg:build && npm run pkg:push && npm run pkg:clean"
  },
  "devDependencies": {
    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
