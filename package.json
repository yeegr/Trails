  "scripts": {
    "postinstall": "react-native link",
    "build": "npm run build-graphics && npm run build-database && npm run build-api",
    "load": "npm run load-uploads && npm load-graphics && npm run load-database && npm run load-api",
    "start": "npm run start-uploads && npm run start-graphics && npm run start-database && npm run start-api",
    "stop": "npm run stop-uploads && npm run stop-graphics && npm run stop-database && npm run stop-api",
    "load-uploads": "docker run -d --network web -p 8080:80 --name uploads -v /Users/Stone/Desktop/trails/uploads:/usr/share/nginx/html:rw nginx",
    "start-uploads": "docker start uploads",
    "stop-uploads": "docker stop uploads",
    "build-graphics": "docker build -t shitulv/graphics -f docker/graphics.dockerfile .",
    "load-graphics": "docker run -d --network web -p 8000:8000 --name content shitulv/graphics",
    "start-graphics": "docker start graphics",
    "stop-graphics": "docker stop graphics",
    "build-database": "docker build -t shitulv/mongo -f docker/mongo.dockerfile .",
    "load-database": "docker run -d --network api --name mongo -v /data/db:/data/db --ip 172.18.0.2 -p 27017:27017 shitulv/mongo --smallfiles",
    "start-database": "docker start mongo",
    "stop-database": "docker stop mongo",
    "build-api": "docker build -t shitulv/api -f docker/api.dockerfile .",
    "load-api": "docker run -d --network api -p 3000:3000 --name api shitulv/api",
    "start-api": "docker start api",
    "stop-api": "docker stop api",
    "remove-api": "docker rm api",
    "delete-api": "docker rmi shitulv/api",
    "reset-api": "npm run stop-api && npm run remove-api && npm run delete-api && webpack && npm run build-api && npm run load-api",
    "ios": "react-native run-ios",
    "lint": "esw webpack.config.* src"
  },
