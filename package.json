{
  "name": "trails",
  "version": "0.8.0",
  "description": "",
  "keywords": [],
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yeegr/trails.git"
  },
  "author": "Stone Chen",
  "license": "ISC",
  "config": {
    "registry": "registry.cn-hangzhou.aliyuncs.com"
  },
  "scripts": {
    "postinstall": "react-native link",

    "update": "ncu -a && ncu -a --packageFile package.api.json && ncu -a --packageFile package.static.json",
    "lint": "esw webpack.config.* src",
    "ios": "react-native run-ios --simulator='iPhone 6 Plus'",
    "codepush": "code-push release-react trails ios",

    "init": "APP_ENV=development",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "build": "webpack && docker-compose build",
    "logs": "docker-compose logs",
    "static": "webpack && docker-compose restart static",
    "api": "webpack && docker-compose restart api",

    "kill": "docker rm -f $(docker ps -aq)",
    "clean": "docker rmi -f $(docker images -qf dangling=true)",
    "empty": "docker rmi -f $(docker images)",

    "create": "docker-compose -f dc.staging.yml build",
    "up": "docker-compose -f dc.staging.yml up -d",
    "push": "docker push $npm_package_config_registry/shitulv/mongodb && docker push $npm_package_config_registry/shitulv/static && docker push $npm_package_config_registry/shitulv/api",
    "remove": "docker rmi $npm_package_config_registry/shitulv/mongodb $npm_package_config_registry/shitulv/static $npm_package_config_registry/shitulv/api",
    "upload": "npm run create && npm run push && npm run remove",
    "staging": "APP_ENV=staging DOCKER_REPO='registry.cn-hangzhou.aliyuncs.com' npm run upload"
  },
  "devDependencies": {
    "browser-sync": "^2.18.5",
    "chai": "^3.5.0",
    "debug": "^2.4.3",
    "eslint": "^3.12.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-watch": "^2.1.14",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "source-map-support": "^0.4.6",
    "webpack": "^1.14.0",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "busboy": "^0.2.13",
    "cookie-parser": "^1.4.3",
    "coordtransform": "^2.1.1",
    "cors": "^2.8.1",
    "errorhandler": "^1.5.0",
    "exif": "^0.6.0",
    "express": "^4.14.0",
    "formidable": "^1.0.17",
    "http-cache": "^0.3.1",
    "immutable": "^3.8.1",
    "jquery": "^3.1.1",
    "jsdom": "^9.8.3",
    "moment": "^2.17.1",
    "moment-duration-format": "^1.3.0",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "numeral": "^2.0.1",
    "path": "^0.12.7",
    "react": "^15.4.1",
    "react-native": "^0.39.2",
    "react-native-app-intro": "^1.1.5",
    "react-native-cacheable-image": "^1.4.3",
    "react-native-camera-roll-picker": "^1.1.9",
    "react-native-chart": "^1.0.8-beta",
    "react-native-code-push": "^1.16.1-beta",
    "react-native-communications": "^2.1.4",
    "react-native-device-info": "^0.9.7",
    "react-native-fs": "^2.0.1-rc.2",
    "react-native-i18n": "^0.1.1",
    "react-native-image-picker": "^0.24.1",
    "react-native-keyboard-spacer": "^0.3.1",
    "react-native-location": "^0.27.0",
