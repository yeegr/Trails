  "license": "ISC",
  "config": {
    "registry": "registry.cn-hangzhou.aliyuncs.com"
  },
  "scripts": {
    "postinstall": "react-native link",
    "update": "ncu -a --packageFile ./docker/config/api.package.json && ncu -a --packageFile ./docker/config/static.package.json",
    "lint": "esw webpack.config.* src",
    "ios": "react-native run-ios --simulator='iPhone 6 Plus'",
    "codepush": "code-push release-react trails ios",
    "init": "export NODE_ENV=development",
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "restart": "docker-compose restart",
    "build": "NODE_ENV=development webpack -d && docker-compose build",
    "logs": "docker-compose logs",
    "static": "webpack -d && docker-compose stop static && docker-compose start static",
    "api": "webpack -d && docker-compose stop api1 && docker-compose start api1",
    "web": "webpack -d && docker-compose stop web1 && docker-compose start web1",
    "gateway": "docker-compose stop gateway && docker-compose start gateway",
    "kill": "docker rm -f $(docker ps -aq)",
    "clean": "docker rmi -f $(docker images -qf since=shitulv/mongodb)",
    "empty": "docker rmi -f $(docker images)",
    "create": "NODE_ENV=staging DOCKER_REPO=$npm_package_config_registry docker-compose -f dc.staging.yml build",
    "up": "docker-compose -f dc.staging.yml up -d",
    "push:mongo": "docker push $npm_package_config_registry/shitulv/mongodb",
    "push:static": "docker push $npm_package_config_registry/shitulv/static",
    "push:api": "docker push $npm_package_config_registry/shitulv/api",
    "push:web": "docker push $npm_package_config_registry/shitulv/web",
    "push:gateway": "docker push $npm_package_config_registry/shitulv/gateway",
    "push": "npm run push:mongo && npm run push:static && npm run push:api && npm run push:web && npm run push:gateway",
    "remove": "docker rmi $npm_package_config_registry/shitulv/mongodb $npm_package_config_registry/shitulv/static $npm_package_config_registry/shitulv/api $npm_package_config_registry/shitulv/web $npm_package_config_registry/shitulv/gateway",
    "upload": "npm run create && npm run push && npm run remove",
    "staging": "NODE_ENV=staging DOCKER_REPO=$npm_package_config_registry webpack -d && npm run upload",
    "api1": "NODE_ENV=staging DOCKER_REPO=$npm_package_config_registry docker-compose -f dc.staging.yml build api1 && docker push $npm_package_config_registry/shitulv/api && docker rmi $npm_package_config_registry/shitulv/api",
    "web1": "NODE_ENV=staging DOCKER_REPO=$npm_package_config_registry docker-compose -f dc.staging.yml build web1 && docker push $npm_package_config_registry/shitulv/web && docker rmi $npm_package_config_registry/shitulv/web"
  },
